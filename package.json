{
  "name": "luoh-blog",
  "type": "module",
  "version": "3.4.8",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "homepage": "https://github.com/luoh-an/blog",
  "scripts": {
    "prepare": "pnpm clean && pnpm build:packages",
    "clean": "pnpx rimraf .data node_modules/.cache",
    "build:packages": "pnpm -r --filter ./packages/* build",
    "dev": "nuxt dev --no-qr",
    "build": "cross-env NODE_OPTIONS='--no-deprecation' nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "stylelint '**/*.{vue,scss}'",
    "lint:fix": "eslint --fix && stylelint '**/*.{vue,scss}' --fix",
    "new": "node scripts/new-blog.ts",
    "check:feed": "pnpx tsx scripts/framework/get-feed.ts",
    "check:feed/all": "pnpx tsx scripts/framework/check-all-feeds.ts",
    "init-moment": "node scripts/init-moment.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "catalog:util",
    "@aws-sdk/s3-request-presigner": "catalog:util",
    "@nuxt/content": "catalog:content",
    "@nuxt/icon": "catalog:ui",
    "@nuxt/image": "catalog:ui",
    "@nuxtjs/color-mode": "catalog:ui",
    "@nuxtjs/seo": "catalog:content",
    "@pinia/nuxt": "catalog:util",
    "@shikijs/colorized-brackets": "catalog:content",
    "@shikijs/transformers": "catalog:content",
    "@vueuse/core": "catalog:util",
    "@vueuse/nuxt": "catalog:util",
    "@vueuse/router": "catalog:util",
    "abcjs": "catalog:content",
    "ci-info": "catalog:framework",
    "date-fns": "catalog:util",
    "date-fns-tz": "catalog:util",
    "dom-to-image": "catalog:",
    "embla-carousel-autoplay": "catalog:ui",
    "embla-carousel-vue": "catalog:ui",
    "embla-carousel-wheel-gestures": "catalog:ui",
    "html2canvas": "catalog:util",
    "jsonwebtoken": "catalog:util",
    "minisearch": "catalog:util",
    "nanoid": "catalog:util",
    "nuxt": "catalog:framework",
    "parse-domain": "catalog:util",
    "plain-shiki": "catalog:content",
    "radash": "catalog:util",
    "shiki": "catalog:content",
    "vue": "catalog:framework",
    "vue-router": "catalog:framework",
    "vue-tippy": "catalog:ui",
    "vue-virtual-scroller": "catalog:ui"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:quality",
    "@clack/prompts": "catalog:util",
    "@dxup/nuxt": "catalog:framework",
    "@iconify-json/catppuccin": "catalog:ui",
    "@iconify-json/icon-park-solid": "catalog:ui",
    "@iconify-json/material-symbols": "catalog:ui",
    "@iconify-json/mynaui": "catalog:ui",
    "@iconify-json/ph": "catalog:ui",
    "@iconify-json/ri": "catalog:ui",
    "@iconify-json/simple-icons": "catalog:ui",
    "@iconify-json/tabler": "catalog:ui",
    "@iconify-json/uim": "catalog:ui",
    "@types/dom-to-image": "catalog:",
    "@types/jsonwebtoken": "catalog:util",
    "@zinkawaii/stylelint-config": "catalog:quality",
    "better-sqlite3": "catalog:content",
    "cross-env": "catalog:framework",
    "cspell": "catalog:quality",
    "eslint": "catalog:quality",
    "fast-xml-parser": "catalog:util",
    "nanoid": "catalog:util",
    "nuxt": "catalog:framework",
    "p-limit": "catalog:util",
    "rehype-katex": "catalog:content",
    "remark-math": "catalog:content",
    "remark-music": "workspace:*",
    "remark-reading-time": "catalog:content",
    "rimraf": "catalog:util",
